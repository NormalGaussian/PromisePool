{
  "name": "promise-pool",
  "version": "1.0.0",
  "description": "Execute async tasks with concurrency control and configurable error handling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "node --import tsx --test src/**/*.test.ts",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "promise",
    "pool",
    "concurrency",
    "async",
    "parallel"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
